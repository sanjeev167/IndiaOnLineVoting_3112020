package com.pon.pvt.voting.entity;

import java.io.Serializable;
import java.util.Date;
import java.util.List;
import javax.persistence.Basic;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.OneToMany;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

import com.pon.pvt.master.entity.AssemblyCandidateMaster;
import com.pon.pvt.master.entity.AssemblyMaster;
import com.pon.pvt.master.entity.LoksabhaMaster;
import com.pon.pvt.master.entity.PollingBoothMaster;
import com.pon.pvt.master.entity.VotersEnrolled;

/**
 *
 * @author Sanjeev
 */
@Entity
@Table(name = "vote_repository")
@NamedQueries({
    @NamedQuery(name = "VoteRepository.findAll", query = "SELECT v FROM VoteRepository v"),
    @NamedQuery(name = "VoteRepository.findById", query = "SELECT v FROM VoteRepository v WHERE v.id = :id"),
    @NamedQuery(name = "VoteRepository.findByVotingPartyId", query = "SELECT v FROM VoteRepository v WHERE v.votingPartyId = :votingPartyId"),
    @NamedQuery(name = "VoteRepository.findByElectionType", query = "SELECT v FROM VoteRepository v WHERE v.electionType = :electionType"),
    @NamedQuery(name = "VoteRepository.findByVotingMode", query = "SELECT v FROM VoteRepository v WHERE v.votingMode = :votingMode"),
    @NamedQuery(name = "VoteRepository.findByVotingDate", query = "SELECT v FROM VoteRepository v WHERE v.votingDate = :votingDate"),
    @NamedQuery(name = "VoteRepository.findByElectionYear", query = "SELECT v FROM VoteRepository v WHERE v.electionYear = :electionYear")})
public class VoteRepository implements Serializable {

    private static final long serialVersionUID = 1L;
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Basic(optional = false)
    @Column(name = "id")
    private Integer id;
    @Column(name = "voting_party_id")
    private Integer votingPartyId;
    @Column(name = "election_type")
    private Character electionType;
    @Column(name = "voting_mode")
    private Integer votingMode;
    @Column(name = "voting_date")
    @Temporal(TemporalType.DATE)
    private Date votingDate=new Date(System.currentTimeMillis());
    @Column(name = "election_year")
    private String electionYear;
    @OneToMany(mappedBy = "voterId")
    private List<MarkOfflineVoter> markOfflineVoterList;
    @OneToMany(mappedBy = "voterId")
    private List<MarkOnlineVoter> markOnlineVoterList;
    @JoinColumn(name = "assembly_candidate_id", referencedColumnName = "id")
    @ManyToOne
    private AssemblyCandidateMaster assemblyCandidateId;
    @JoinColumn(name = "assembly_id", referencedColumnName = "id")
    @ManyToOne
    private AssemblyMaster assemblyId;
    @JoinColumn(name = "loksabha_id", referencedColumnName = "id")
    @ManyToOne
    private LoksabhaMaster loksabhaId;
    @JoinColumn(name = "polling_booth_id", referencedColumnName = "id")
    @ManyToOne
    private PollingBoothMaster pollingBoothId;
    @JoinColumn(name = "voter_id", referencedColumnName = "id")
    @ManyToOne
    private VotersEnrolled voterId;

    public VoteRepository() {
    }

    public VoteRepository(Integer id) {
        this.id = id;
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getVotingPartyId() {
        return votingPartyId;
    }

    public void setVotingPartyId(Integer votingPartyId) {
        this.votingPartyId = votingPartyId;
    }

    public Character getElectionType() {
        return electionType;
    }

    public void setElectionType(Character electionType) {
        this.electionType = electionType;
    }

    public Integer getVotingMode() {
        return votingMode;
    }

    public void setVotingMode(Integer votingMode) {
        this.votingMode = votingMode;
    }

    public Date getVotingDate() {
        return votingDate;
    }

    public void setVotingDate(Date votingDate) {
        this.votingDate = votingDate;
    }

    public String getElectionYear() {
        return electionYear;
    }

    public void setElectionYear(String electionYear) {
        this.electionYear = electionYear;
    }

    public List<MarkOfflineVoter> getMarkOfflineVoterList() {
        return markOfflineVoterList;
    }

    public void setMarkOfflineVoterList(List<MarkOfflineVoter> markOfflineVoterList) {
        this.markOfflineVoterList = markOfflineVoterList;
    }

    public List<MarkOnlineVoter> getMarkOnlineVoterList() {
        return markOnlineVoterList;
    }

    public void setMarkOnlineVoterList(List<MarkOnlineVoter> markOnlineVoterList) {
        this.markOnlineVoterList = markOnlineVoterList;
    }

    public AssemblyCandidateMaster getAssemblyCandidateId() {
        return assemblyCandidateId;
    }

    public void setAssemblyCandidateId(AssemblyCandidateMaster assemblyCandidateId) {
        this.assemblyCandidateId = assemblyCandidateId;
    }

    public AssemblyMaster getAssemblyId() {
        return assemblyId;
    }

    public void setAssemblyId(AssemblyMaster assemblyId) {
        this.assemblyId = assemblyId;
    }

    public LoksabhaMaster getLoksabhaId() {
        return loksabhaId;
    }

    public void setLoksabhaId(LoksabhaMaster loksabhaId) {
        this.loksabhaId = loksabhaId;
    }

    public PollingBoothMaster getPollingBoothId() {
        return pollingBoothId;
    }

    public void setPollingBoothId(PollingBoothMaster pollingBoothId) {
        this.pollingBoothId = pollingBoothId;
    }

    public VotersEnrolled getVoterId() {
        return voterId;
    }

    public void setVoterId(VotersEnrolled voterId) {
        this.voterId = voterId;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (id != null ? id.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof VoteRepository)) {
            return false;
        }
        VoteRepository other = (VoteRepository) object;
        if ((this.id == null && other.id != null) || (this.id != null && !this.id.equals(other.id))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "com.example.demo.VoteRepository[ id=" + id + " ]";
    }
    
}
